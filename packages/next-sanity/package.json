    "@sanity/webhook": "4.0.4",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-v8": "^3.2.4",
    "next": "catalog:",
    "publint": "^0.3.13",
    "react": "catalog:",
    "react-dom": "catalog:",
    "styled-components": "catalog:",
    "tsdown": "0.15.4",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@sanity/client": "^7.11.2",
    "next": "^15.1.0-0",
    "react": "^18.3 || ^19",
    "react-dom": "^18.3 || ^19",
    "sanity": "^4.10.1",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  }
