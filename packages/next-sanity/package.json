{
  "name": "next-sanity",
  "version": "13.0.0-cache-components.24",
  "description": "Sanity.io toolkit for Next.js",
  "keywords": [
    "live",
    "next.js",
    "preview",
    "sanity",
    "sanity.io",
    "studio",
    "studio-v3"
  ],
  "homepage": "https://github.com/sanity-io/next-sanity#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/next-sanity/issues"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/next-sanity.git",
    "directory": "packages/next-sanity"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "imports": {
    "#env": {
      "next-js": "./src/_env/next-js.ts",
      "react-server": "./src/_env/react-server.ts",
      "default": "./src/_env/default.ts"
    },
    "#live/*": "./src/live/shared/*.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./draft-mode": "./src/draft-mode/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./image": "./src/image/index.ts",
    "./live": {
      "next-js": "./src/live/conditions/next-js/index.ts",
      "react-server": "./src/live/conditions/react-server/index.ts",
      "default": "./src/live/conditions/default/index.ts"
    },
    "./live/cache-life": "./src/live/cache-life.ts",
    "./live/client-components": "./src/live/client-components/index.ts",
    "./live/server-actions": {
      "next-js": "./src/live/server-actions/index.next-js.ts",
      "default": "./src/live/server-actions/index.default.ts"
    },
    "./studio": "./src/studio/index.ts",
    "./studio/client-component": "./src/studio/client-component/index.ts",
    "./visual-editing": "./src/visual-editing/index.ts",
    "./visual-editing/client-component": "./src/visual-editing/client-component/index.ts",
    "./visual-editing/server-actions": "./src/visual-editing/server-actions/index.ts",
    "./webhook": "./src/webhook/index.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./draft-mode": "./dist/draft-mode/index.js",
      "./hooks": "./dist/hooks/index.js",
      "./image": "./dist/image/index.js",
      "./live": {
        "next-js": "./dist/live/conditions/next-js/index.js",
        "react-server": "./dist/live/conditions/react-server/index.js",
        "default": "./dist/live/conditions/default/index.js"
      },
      "./live/cache-life": "./dist/live/cache-life.js",
      "./live/client-components": "./dist/live/client-components/index.js",
      "./live/server-actions": {
        "next-js": "./dist/live/server-actions/index.next-js.js",
        "default": "./dist/live/server-actions/index.default.js"
      },
      "./studio": "./dist/studio/index.js",
      "./studio/client-component": "./dist/studio/client-component/index.js",
      "./visual-editing": "./dist/visual-editing/index.js",
      "./visual-editing/client-component": "./dist/visual-editing/client-component/index.js",
      "./visual-editing/server-actions": "./dist/visual-editing/server-actions/index.js",
      "./webhook": "./dist/webhook/index.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown --publint",
    "coverage": "npm test -- --coverage",
    "dev": "tsdown --watch",
    "prepack": "tsdown",
    "test": "vitest"
  },
  "dependencies": {
    "@portabletext/react": "^6.0.2",
    "@sanity/client": "^7.14.1",
    "@sanity/comlink": "^4.0.1",
    "@sanity/presentation-comlink": "^2.0.1",
    "@sanity/preview-url-secret": "^4.0.3",
    "@sanity/visual-editing": "^5.2.1",
    "@sanity/webhook": "^4.0.4",
    "dequal": "^2.0.3",
    "groq": "^5.8.1",
    "history": "^5.3.0"
  },
  "devDependencies": {
    "@sanity/tsconfig": "catalog:",
