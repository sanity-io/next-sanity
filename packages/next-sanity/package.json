      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "live": [
        "./dist/live.d.ts"
      ],
      "experimental/live": [
        "./dist/experimental/live.d.ts"
      ],
      "experimental/client-components/live": [
        "./dist/experimental/client-components/live.d.ts"
      ],
      "image": [
        "./dist/image/index.d.ts"
      ],
      "studio": [
        "./dist/studio/index.d.ts"
      ],
      "studio/client-component": [
        "./dist/studio/client-component/index.d.ts"
      ],
      "visual-editing": [
        "./dist/visual-editing/index.d.ts"
      ],
      "visual-editing/client-component": [
        "./dist/visual-editing/client-component/index.d.ts"
      ],
      "visual-editing/server-actions": [
        "./dist/visual-editing/server-actions/index.d.ts"
      ],
      "webhook": [
        "./dist/webhook/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown --publint",
    "coverage": "npm test -- --coverage",
    "dev": "tsdown --watch",
    "prepublishOnly": "tsdown",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "dependencies": {
    "@portabletext/react": "^4.0.3",
    "@sanity/client": "catalog:",
    "@sanity/comlink": "^3.0.9",
    "@sanity/presentation-comlink": "^1.0.29",
    "@sanity/preview-url-secret": "catalog:",
    "@sanity/visual-editing": "^3.0.5",
    "dequal": "^2.0.3",
    "groq": "^4.10.1",
    "history": "^5.3.0",
    "server-only": "^0.0.1",
    "use-effect-event": "^2.0.3"
  },
  "devDependencies": {
    "@sanity/browserslist-config": "^1.0.5",
    "@sanity/pkg-utils": "^8.1.17",
