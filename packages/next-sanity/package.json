    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "js-yaml": "^4.1.1",
    "next": "catalog:",
    "publint": "^0.3.17",
    "react": "catalog:",
    "react-dom": "catalog:",
    "styled-components": "catalog:",
    "tsdown": "0.20.3",
    "typescript": "catalog:",
    "vitest": "^4.0.18",
    "vitest-package-exports": "^1.2.0"
  },
  "peerDependencies": {
    "@sanity/client": "^7.14.1",
    "next": "^16.0.0-0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanity": "^5.8.1",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  }
